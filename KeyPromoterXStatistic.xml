<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Commit...">
        <Item>
          <IdeaActionID>CheckinProject</IdeaActionID>
          <ShortCut>'Ctrl+K'</ShortCut>
          <Description>Commit...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Copy Path">
        <Item>
          <IdeaActionID>CopyPaths</IdeaActionID>
          <ShortCut>'Ctrl+Shift+C'</ShortCut>
          <Description>Copy Path</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Delete">
        <Item>
          <IdeaActionID>FileChooser.Delete</IdeaActionID>
          <ShortCut>'Delete'</ShortCut>
          <Description>Delete</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Project Structure...">
        <Item>
          <IdeaActionID>ShowProjectStructureSettings</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Shift+S'</ShortCut>
          <Description>Project Structure...</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'Solution.main()'">
        <Item>
          <IdeaActionID>RunClass</IdeaActionID>
          <ShortCut>'Ctrl+Shift+F10'</ShortCut>
          <Description>Run 'Solution.main()'</Description>
          <Count>5</Count>
        </Item>
      </Statistic>
      <Statistic Action="Settings...">
        <Item>
          <IdeaActionID>ShowSettings</IdeaActionID>
          <ShortCut>'Ctrl+Alt+S'</ShortCut>
          <Description>Settings...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>